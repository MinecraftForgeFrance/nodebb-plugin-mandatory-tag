{
  "id": "nodebb-plugin-mandatory-tag",
  "url": "https://github.com/MinecraftForgeFrance/nodebb-plugin-mandatory-tag",
  "library": "./library.js",
  "templates": "./static/templates",
  "acpScripts": [
    "static/js/admin.js"
  ],
  "hooks": [
    { "hook": "static:app.load", "method":"init" },
    { "hook": "filter:admin.header.build", "method": "addToAdminNav" },
    { "hook": "filter:topic.post", "method": "topicPostOrEdit" },
    { "hook": "filter:topic.edit", "method": "topicPostOrEdit" }
  ],
  "languages": "languages"
}
